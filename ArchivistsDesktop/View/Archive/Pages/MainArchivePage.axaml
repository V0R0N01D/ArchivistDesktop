<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 FontFamily="Verdana"
             x:Class="ArchivistsDesktop.View.ArchiveWindow.Pages.MainArchivePage">
	<Grid
		RowDefinitions="*,*,*"
		ColumnDefinitions="*,*">
		<TextBlock
			Text="Архив"
			Grid.ColumnSpan="3"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			FontSize="40"/>
		<Button
			Name="ListStudent"
			Grid.Row="1"
			Margin="10"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Content="Просмотр списка студентов"/>
		<Button
			Name="ListGroup"
			Grid.Row="1"
			Grid.Column="1"
			Margin="10"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Content="Просмотр списка групп"/>
	</Grid>
</UserControl>
